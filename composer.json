{
	"name": "lionar/messages",
	"description": "the base rules for lionar messages.",
	"keywords": [ "Lionar", "Messages" ],
	"license": "MIT",
	"require" : {
		"lionar/support": "*",
		"php": ">=5.5.9"
  	},
	"require-dev": {
		"lionar/testing": "*"
	},
	"autoload": {
    	"psr-4": {
			"Messages\\": "source"
    	}
  	},
	"autoload-dev": {
		"psr-4": {
			"Messages\\Tests\\": "tests"
		}
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
